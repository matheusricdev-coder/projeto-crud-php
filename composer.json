{
    "name": "mosyle/coffee-api",
    "description": "Coffee API - PHP Backend Challenge for Mosyle",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "Matheus Ricardo",
            "email": "matheusricdevo@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit",
        "serve": "php -S localhost:8000"
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    }
}

